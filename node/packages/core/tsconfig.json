{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": "./lib",
    "alwaysStrict": false,
    "noImplicitUseStrict": true,
    "paths": {
      "@interfaces/*": [
        "src/interfaces/*"
      ],
      "@data/*": [
        "src/data/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ],
      "@api/*": [
        "src/api/*"
      ]
    }
  },
  "include": [
    "src/**/*"
  ]
}